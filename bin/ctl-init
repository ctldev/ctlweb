#!/usr/bin/env python3
import os
import sys
import getopt
lib_path = os.getcwd() + "/../lib/backend/util"
sys.path.append( lib_path )
from log import Log

opt = "m:"
long_opt = ["module="]
max_output_level = 2

def usage():
    print("Usage:", sys.argv[0], """[-m <modul>| --module=<module>]""")


def main():
    Log.streamoutput()
    opts, args = getopt.getopt(sys.argv[1:], opt, long_opt)
    for o, a in opts:
        if o in ("-m", "--module"):
            print("module "+a+" wird gestartet")

if __name__ == "__main__":      
    main()
